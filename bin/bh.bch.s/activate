echo "[$ . bh.bch activate]:"

_export () { export $1=$2; }
_export BH_BCH_HISTORY      ${STEVE}/__BRYAN__
_export BH_BCH_HISTORY_SRC  git@github.com:bryanhann/bch.history
_report () {
    echo "    \| $*"
}
for xx in $(set | grep "BH_BCH_"); do
    _report "$xx"
done

steve clone4dst4src $BH_BCH_HISTORY $BH_BCH_HISTORY_SRC
source bh.bch prefs

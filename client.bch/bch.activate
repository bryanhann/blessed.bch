#!/path/to/sh
export BCH_HISTORY=${HOME}/.__bch__
main () {
    local S=git@github.com:bryanhann/bch.history
    local D=$BCH_HISTORY
    [ -d $D ] || echo "[blessed.bch]: installing BCH-HISTORY"
    [ -d $D ] || git clone ${S} ${D}
}

echo "[blessed.bch]: BCH_HISTORY == [$BCH_HISTORY]"

poet.bless -i zjot
export ZJOT_ACC=${BCH_HISTORY}/zjot/acc
